[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-order-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create_rows": [
            [
              "add",
              "_id",
              [
                "string",
                "_S.id"
              ]
            ],
            [
              "add",
              "id",
              "_S.id"
            ],
            [
              "add",
              "$original_id",
              "_R._S._id"
            ],
            [
              "add",
              "version",
              "_P._S.version"
            ]
          ],
          "default": [
            [
              "create-child",
              [
                "apply",
                "create_rows",
                "_S.orderLines"
              ]
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all2",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "transform": [
      {
        "type": "emit_children"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "exclude_completeness": [
      "{{@ system @}}-{{@ datatype @}}-share"
    ],
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all2",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "filter",
              [
                "neq",
                "_S._deleted",
                true
              ]
            ],
            [
              "copy",
              "*"
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "{{@ datatype @}}-lookup",
        "replace_entity": true,
        "side_effects": false,
        "system": "{{@ system @}}",
        "type": "rest"
      },
      {
        "rules": {
          "default": [
            [
              "comment",
              "TODO should use id-expression and payload-property in rest transform"
            ],
            [
              "add",
              "_id",
              [
                "string",
                "_S.value.id"
              ]
            ],
            [
              "merge",
              "_S.value"
            ],
            [
              "merge",
              [
                "max",
                "_.",
                [
                  "apply",
                  "last-modified",
                  "_S.value.changes"
                ]
              ]
            ],
            [
              "add",
              "changes",
              [
                "filter",
                [
                  "eq",
                  "_.changeType",
                  "CREATE"
                ],
                "_S.value.changes"
              ]
            ]
          ],
          "last-modified": [
            [
              "filter",
              [
                "in",
                "_S.changeType",
                [
                  "list",
                  "UPDATE",
                  "CREATE"
                ]
              ]
            ],
            [
              "add",
              "$last-modified",
              [
                "datetime-parse",
                "%Y-%m-%dT%H:%M:%S%z",
                "_S.timestamp"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "primary_key": "id",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-collect-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "batch_size": 1,
    "namespaced_identifiers": false,
    "sink": {
      "deletion_tracking": false,
      "set_initial_offset": "onload"
    },
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-transform",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "comment",
              "adding below dtl to remove order.id for updates. ref. (IS-14784)"
            ],
            [
              "copy",
              "*"
            ],
            [
              "if",
              [
                "and",
                [
                  "neq",
                  "_S._deleted",
                  true
                ],
                [
                  "is-not-null",
                  "_S.id"
                ],
                [
                  "is-not-null",
                  "_S.order"
                ]
              ],
              [
                "add",
                "$payload",
                [
                  "apply",
                  "update-payload",
                  "_S."
                ]
              ],
              [
                "add",
                "$payload",
                [
                  "apply",
                  "insert-payload",
                  "_S."
                ]
              ]
            ]
          ],
          "insert-payload": [
            [
              "merge",
              [
                "dict",
                [
                  "filter",
                  [
                    "not",
                    [
                      "or",
                      [
                        "matches",
                        "_*",
                        [
                          "first",
                          "_."
                        ]
                      ],
                      [
                        "matches",
                        "$*",
                        [
                          "first",
                          "_."
                        ]
                      ]
                    ]
                  ],
                  [
                    "items",
                    "_S."
                  ]
                ]
              ]
            ]
          ],
          "update-payload": [
            [
              "merge",
              [
                "dict",
                [
                  "filter",
                  [
                    "not",
                    [
                      "or",
                      [
                        "matches",
                        "_*",
                        [
                          "first",
                          "_."
                        ]
                      ],
                      [
                        "matches",
                        "$*",
                        [
                          "first",
                          "_."
                        ]
                      ],
                      [
                        "eq",
                        "order",
                        [
                          "first",
                          "_."
                        ]
                      ]
                    ]
                  ],
                  [
                    "items",
                    "_S."
                  ]
                ]
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "operation_delete": "{{@ datatype @}}-delete",
          "operation_insert": "{{@ datatype @}}-insert",
          "operation_lookup": "{{@ datatype @}}-lookup",
          "operation_update": "{{@ datatype @}}-update",
          "primary_key": "id",
          "rest_system": "{{@ system @}}",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-share-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  }
]